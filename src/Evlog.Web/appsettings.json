{
  "AppSettings": {
    "DatabaseProvider": "MySql"
  },

  "MySql": {
    "ConnectionString": "--mysql-connection-string--"
  },

  "Email": {
    "FromName": "Evlog Admin",
    "FromEmail": "noreply@example.com",
    "Provider": "Log"
  },

  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [ "Console" ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "evlog"
    }
  }
}
